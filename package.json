{
  "name": "publications-retriever",
  "homepage": "https://github.com/dataesr/publications-finder/",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "client",
    "server"
  ],
  "dependencies": {
    "concurrently": "^8.0.1",
    "primereact": "^9.6.0"
  },
  "scripts": {
    "build-fullstack": "npm -w client run build -- --mode ${npm_config_mode} --emptyOutDir --outDir ../server/dist",
    "client": "npm -w client run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "preview": "npm run build --mode=${npm_config_mode} && npm -w server start",
    "server": "npm -w server run dev",
    "start": "npm run dev",
    "build-client": "npm -w client run build",
    "preview-client": "npm run build-client && npm -w client run preview",
    "build": "vite build"
  }
}
