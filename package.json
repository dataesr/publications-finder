{
  "name": "publications-retriever",
  "homepage": "https://github.com/dataesr/publications-finder/",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "client",
    "server"
  ],
  "dependencies": {
    "concurrently": "^8.0.1",
    "primereact": "^9.6.0",
    "react-tooltip": "^5.18.1"
  },
  "scripts": {
    "build-fullstack": "npm -w client run build -- --mode ${npm_config_mode}",
    "client": "npm -w client run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "preview-fullstack": "npm run build --mode=${npm_config_mode} && npm -w server start",
    "server": "npm -w server run dev",
    "start": "npm run dev",
    "preview": "vite build && vite preview",
    "build": "vite build"
  }
}
